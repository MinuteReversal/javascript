{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "mock",
      "request": "launch",
      "name": "Mock Debug(.md file)",
      "program": "${workspaceFolder}/${relativeFile}",
      "stopOnEntry": true
    },
    {
      "name": "Launch Chrome(network)",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://192.168.31.171/js/${relativeFile}",
      "webRoot": "${cwd}"
    },
    {
      //https://github.com/Microsoft/vscode-chrome-debug
      "name": "Launch Chrome(local)",
      "type": "chrome",
      "request": "launch",
      "file": "${file}"
    }
  ]
}
